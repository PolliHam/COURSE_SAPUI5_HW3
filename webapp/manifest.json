{
	"_version": "1.12.0",
	"sap.app": {
		"id": "ZSHN.COURSE_SAPUI5",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "ZSHN.COURSE_SAPUI5.view.Form",
			"type": "XML",
			"async": true,
			"id": "Form"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ZSHN.COURSE_SAPUI5.i18n.i18n"
				}
			},
			"accounts": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/accounts.json"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "ZSHN.COURSE_SAPUI5.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "RouteForm",
					"pattern": "RouteForm",
					"target": [
						"TargetForm"
					]
				},
				{
					"name": "Inputs",
					"pattern": "Inputs",
					"target": [
						"Inputs"
					]
				},
				{
					"name": "Form",
					"pattern": "Form",
					"target": [
						"Form"
					]
				},
				{
					"name": "Table",
					"pattern": "Table",
					"target": [
						"Table"
					]
				}
			],
			"targets": {
				"TargetForm": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "Form",
					"viewName": "Form"
				},
				"Inputs": {
					"viewType": "XML",
					"viewName": "Inputs"
				},
				"Form": {
					"viewType": "XML",
					"viewName": "Form"
				},
				"Table": {
					"viewType": "XML",
					"viewName": "Table"
				},
				"test": {
					"viewType": "XML",
					"viewName": "test"
				}
			}
		}
	}
}